{
  "name": "ReMind",
  "private": true,
  "version": "0.0.9",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck:base": "tsc -p tsconfig.json --noEmit",
    "typecheck:strict": "tsc -p tsconfig.strict.json --noEmit",
    "typecheck": "npm run typecheck:strict",
    "lint": "eslint .",
    "preview": "vite preview",
    "release": "standard-version",
    "openapi:generate": "python scripts/generate_openapi_ts.py",
    "openapi:check": "python scripts/generate_openapi_ts.py --check",
    "test:unit": "vitest run",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:install": "playwright install --with-deps chromium"
  },
  "standard-version": {
    "infile": "docs/CHANGELOG.md"
  },
  "dependencies": {
    "canvas-confetti": "^1.9.3",
    "chart.js": "^4.4.0",
    "d3": "^7.8.5",
    "dompurify": "^3.3.0",
    "i18next": "^25.8.13",
    "katex": "^0.16.25",
    "lucide-react": "^0.555.0",
    "markdown-it": "^14.1.0",
    "mermaid": "^11.12.2",
    "nomnoml": "^1.7.0",
    "prismjs": "^1.30.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.0",
    "react-router-dom": "^7.13.0"
  },
  "overrides": {
    "lodash": "4.17.23",
    "lodash-es": "4.17.23"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/canvas-confetti": "^1.6.4",
    "@types/d3": "^7.4.3",
    "@types/dompurify": "^3.0.5",
    "@types/katex": "^0.16.7",
    "@types/markdown-it": "^14.1.2",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.2.4"
  }
}
